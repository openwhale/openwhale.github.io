{
	"extends": [
		"airbnb",
		"prettier",
		"prettier/react"
	],
	"parser": "babel-eslint",
	"env": {
		"browser": true
	},
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"singleQuote": true,
				"useTabs": true
			}
		]
	},
	"plugins": [
		"prettier"
	],
	"settings": {
		"import/resolver": {
			"babel-module": {}
		}
	}
}